<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="470.0" prefWidth="927.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="cn.dominic.controller.ManageController">
    <children>
        <TableView fx:id="tableView" layoutY="165.0" prefHeight="304.0" prefWidth="938.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="200.0" text="名称"/>
                <TableColumn fx:id="unit" prefWidth="200.0" resizable="false" text="单位"/>
                <TableColumn fx:id="number" prefWidth="200.0" text="数量"/>
                <TableColumn fx:id="price" prefWidth="200.0" text="单价"/>
<!--                <TableColumn fx:id="totalPrice" prefWidth="72.0" text="总价"/>-->
            </columns>
        </TableView>

        <Label layoutX="288.0" layoutY="78.0" text="搜索名称:">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <TextField fx:id="selectName" layoutX="388.0" layoutY="78.0"/>

        <Button fx:id="btnInsert" layoutX="386.0" layoutY="121.0" mnemonicParsing="false" onMouseClicked="#insert"
                prefHeight="35.0" prefWidth="62.0" text="新增" textAlignment="CENTER" textFill="#439ac9">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="btnUpdate" layoutX="486.0" layoutY="121.0" mnemonicParsing="false"
                onMouseClicked="#update" prefHeight="27.0" prefWidth="62.0" text="修改" textFill="#439ac9">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="btnSelect" layoutX="586.0" layoutY="121.0" mnemonicParsing="false" onMouseClicked="#select"
                prefHeight="35.0" prefWidth="62.0" styleClass="interfaceButton" text="查询" textFill="#439ac9">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="btnDelete" layoutX="686.0" layoutY="121.0" mnemonicParsing="false" onMouseClicked="#delete"
                prefHeight="35.0" prefWidth="62.0" styleClass="interfaceButton" text="删除" textFill="#439ac9">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Hyperlink layoutX="867.0" layoutY="6.0" onMouseClicked="#back" text="&lt;-返回">
            <font>
                <Font size="14.0"/>
            </font>
        </Hyperlink>
    </children>
</AnchorPane>
